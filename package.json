{
  "name": "nosleepjavascript-blog",
  "private": true,
  "description": "A programming blog for insomniac",
  "version": "0.1.0",
  "author": "the gang",
  "scripts": {
    "deploy": "gatsby build --prefix-paths && gh-pages -d public -b master",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "prettier --check \"**/*.{js,jsx,json,md}\""
  },
  "dependencies": {
    "@emotion/core": "10.0.27",
    "@emotion/styled": "10.0.27",
    "color": "3.1.2",
    "gatsby": "^2.18.4",
    "gatsby-image": "^2.2.37",
    "gatsby-plugin-emotion": "4.1.21",
    "gatsby-plugin-feed": "^2.3.22",
    "gatsby-plugin-google-analytics": "^2.1.29",
    "gatsby-plugin-manifest": "^2.2.30",
    "gatsby-plugin-offline": "^3.0.24",
    "gatsby-plugin-react-helmet": "^3.1.16",
    "gatsby-plugin-sharp": "^2.3.4",
    "gatsby-plugin-typography": "^2.3.18",
    "gatsby-remark-autolink-headers": "2.1.21",
    "gatsby-remark-copy-linked-files": "^2.1.31",
    "gatsby-remark-emojis": "^0.4.2",
    "gatsby-remark-images": "^3.1.34",
    "gatsby-remark-prismjs": "^3.3.25",
    "gatsby-remark-reading-time": "1.1.0",
    "gatsby-remark-responsive-iframe": "^2.2.28",
    "gatsby-remark-smartypants": "^2.1.17",
    "gatsby-remark-table-of-contents": "0.0.9",
    "gatsby-source-filesystem": "^2.1.39",
    "gatsby-transformer-remark": "^2.6.39",
    "gatsby-transformer-sharp": "^2.3.6",
    "prismjs": "^1.17.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-typography": "^0.16.19",
    "typeface-merriweather": "0.0.72",
    "typeface-montserrat": "0.0.75",
    "typography": "^0.16.19",
    "typography-theme-fairy-gates": "^0.16.19",
    "typography-theme-github": "0.16.19",
    "typography-theme-wordpress-2016": "^0.16.19"
  },
  "devDependencies": {
    "gatsby-transformer-yaml": "2.2.20",
    "gh-pages": "^2.1.1",
    "husky": ">=1",
    "lint-staged": ">=8",
    "prettier": "^1.19.1"
  },
  "license": "MIT",
  "main": "n/a",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
